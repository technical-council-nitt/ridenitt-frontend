{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://ridenitt-tc.onrender.com/api/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "https://ridenitt-tc.onrender.com/auth/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
